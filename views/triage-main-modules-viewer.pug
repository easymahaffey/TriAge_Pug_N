    section !{ textContent }
    //- - ${ textContent }
    //- ${textContent}

    h4 #{ questionNumber }
    //- h4 Question 1 of 1
    h4 #{ questionSubTitle }
      //- span#timerDisplay Time remaining: #{ timer }
    br
    if questionText
      p#question #{ questionText }
      br
      p#answer Answer below:
      form(action="/secureMainChallengeModulesS2aq1/" + id method="post")
        input(type="text" name="student_id" value=student_id hidden)
        input(type="text" name="answerDate" hidden)
        input(type="text" name="questionGroupModule" value=questionGroupModule hidden)
        input(type="text" name="questionModule" value=questionModule hidden)
        input(type="text" name="questionTitle" value=questionTitle hidden)
        input(type="text" name="answerTime" hidden)
        input(type="text" name="questionNumber" value=questionNumber hidden)
        label(for="questionText") 
        input(type="text" name="questionText" id="questionText" value=questionText hidden)
        input.answerArea(type="textarea" name="answerText" placeholder=questionSubTitle)
        br
        button.btn.btn-primary.btn-lg.btn-module(type="submit") Submit
      br

