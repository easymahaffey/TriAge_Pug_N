  section.container-2
    h2 #{ textTitle } (#{ textNumber })
    //- h2 #{ questionTitle } (#{ textNumber } #{ questionNumber }) 
    //- h2 TriAge Challenge Education (S2aQ1)
    h3 #{ textSubTitle }
    //- h3 Science of Stress
    
    //- p Member Id: #{ info }
    //- p Admin Id: #{ id } / #{ name }
    //- p Text Id: #{ text_id }
    //- p Text Title: #{ textTitle } #{ textNumber}
    //- p Text Sub Title: #{ textSubTitle }
