				a.nav-link.dropdown-toggle(href="#" #navbarDropdown role="button" data-toggle="dropdown"
				aria-haspopup="true" aria-expanded="false") Peer Training

				div.dropdown-menu(aria-labelledby="navbarDropdown") 
					a.dropdown-item(href="/triage-peer-training/" + id) Peer Training
					a.dropdown-item(href="/triage-my-peer-training/" + id) My Peer Training
					if member_student_status === "Active"
						a.dropdown-item(href="/member-peer-training/" + id) Peer Training Modules
						//- a.dropdown-item(href="/triage-main-challenge-modules/" + id) Challenge Modules
						a.dropdown-item(href="/studentAnswers/" + id) Peer Training Responses

