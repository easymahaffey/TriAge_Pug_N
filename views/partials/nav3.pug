nav.navbar.navbar-expand-lg.navbar-dark.bg-dark.text-white
	span
		img(src="/assets/images/logos/logo.jpg" alt="logo" height= 60)
		a.navbar-brand(href="/finalLandingMember/" + id) TriAge Project (#{ membership_role } #{ student_role })

	span
		//- input(type="button", name="dark_mode" value="true")
		//- button.btn.btn-dark.btn-outline-secondary.text-white.mr-3(onclick="memberDarkToggle()") Dim

		button.navbar-toggler(type="button"
			data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation")
			span.navbar-toggler-icon

	div.collapse.navbar-collapse#navbarSupportedContent
		ul.navbar-nav.mr-auto

			li.nav-item.dropdown
				a.nav-link.dropdown-toggle(href="#" #navbarDropdown role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false") Home
					span.sr-only (current)
		
				div.dropdown-menu(aria-labelledby="navbarDropdown")
					a.dropdown-item(href="/finalLandingMember/" + id) Home
					hr 
					if student_loggedIn
						a.dropdown-item(href="/logoutStudent/" + id) Student Logout
					else if loggedIn
						a.dropdown-item(href="/logoutMember/" + id) Member Logout

			if membership_role === 'Local Administrator' ||membership_role === 'Site Manager'
				li.nav-item.dropdown
					a.nav-link.dropdown-toggle(href="#" #navbarDropdown role="button" data-toggle="dropdown"
					aria-haspopup="true" aria-expanded="false") Admin
							
					div.dropdown-menu(aria-labelledby="navbarDropdown") 
						a.dropdown-item(href="/memberDatabase/" + id) Member Database
						a.dropdown-item(href="/vendorDatabase/" + id) Services Database
						if membership_role === 'Local Administrator'
							hr
							a.dropdown-item(href="/login/" + id) Admin Login            
            
			li.nav-item.dropdown
				a.nav-link.dropdown-toggle(href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false") Projects

				div.dropdown-menu(aria-labelledby="navbarDropdown")
					a.dropdown-item(href="/triage-project-az-phoenix/" + id) Phoenix
					a.dropdown-item(href="/triage-project-az-tucson/" + id) Tucson

			li.nav-item.dropdown
				a.nav-link.dropdown-toggle(href="#" #navbarDropdown role="button" data-toggle="dropdown"
				aria-haspopup="true" aria-expanded="false") About Us
						
				div.dropdown-menu(aria-labelledby="navbarDropdown") 
					a.dropdown-item(href="/company-profile/" + id) Project Profile
					a.dropdown-item(href="/company-events/" + id ) Project Events

			li.nav-item.dropdown
				a.nav-link.dropdown-toggle(href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false") In The News

				div.dropdown-menu(aria-labelledby="navbarDropdown")
					a.dropdown-item(href="/local-news/" + id) Local
					a.dropdown-item(href="/national-news/" + id) National

			li.nav-item.dropdown
				a.nav-link.dropdown-toggle(href="#" #navbarDropdown role="button" data-toggle="dropdown"
				aria-haspopup="true" aria-expanded="false") Challenge

				div.dropdown-menu(aria-labelledby="navbarDropdown") 
					a.dropdown-item(href="/triage-challenge/" + id) Challenge
					a.dropdown-item(href="/triage-my-challenge/" + id) My Challenge
					if member_student_status === "Active"
						a.dropdown-item(href="/member-challenge/" + id) Challenge Modules
						//- a.dropdown-item(href="/triage-main-challenge-modules/" + id) Challenge Modules
						a.dropdown-item(href="/studentAnswers/" + id) Challenge Responses

